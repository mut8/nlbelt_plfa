<text:p text:style-name="ttRed">&gt;  source(&quot;/home/lluc/R_functions/functions.R&quot;)</text:p>

<text:p text:style-name="ttRed">&gt;  source(&quot;~/R_functions/plfaplotfunction.R&quot;)</text:p>

<text:p text:style-name="ttRed">&gt;  mols &lt;- data.frame(read.csv(&quot;/home/lluc/Dropbox/MUN/phd/results/NL-BELT/Batches21-25combined/R/mols.csv&quot;))</text:p>

<text:p text:style-name="ttRed">&gt;  fames &lt;- read.csv(&quot;/home/lluc/Dropbox/MUN/phd/results/NL-BELT/Batches21-25combined/R/fames.csv&quot;)</text:p>

<text:p text:style-name="ttRed">&gt;  samples &lt;- read.csv(&quot;/home/lluc/Dropbox/MUN/phd/results/NL-BELT/Batches21-25combined/R/samples.csv&quot;)</text:p>

<text:p text:style-name="ttRed">&gt;  cond.rel &lt;- samples$trusted_rel == T</text:p>

<text:p text:style-name="ttRed">&gt;  cond.abs &lt;- samples$trusted_abs == T</text:p>

<text:p text:style-name="ttRed">&gt;  cond.ref &lt;- samples$Region != &quot;Ref&quot;</text:p>

<text:p text:style-name="ttRed">&gt;  cond1 &lt;- cond.abs &amp; cond.ref</text:p>

